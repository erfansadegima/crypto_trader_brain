{
  "name": "Crypto Trading Brain - Webhook",
  "nodes": [
    {
      "parameters": {
        "cronExpression": "0 */1 * * * *"
      },
      "name": "Every Minute",
      "type": "n8n-nodes-base.cron",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "requestMethod": "POST",
        "url": "http://YOUR_HOST:8000/webhook/batch",
        "jsonParameters": true,
        "options": {
          "bodyContentType": "json"
        },
        "body": {
          "key": "={{ $json[\"body\"] || {\"symbol\": \"BTC/USDT\", \"timeframe\": \"1h\"} }}"
        }
      },
      "name": "Call Trading Brain",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [520, 300]
    }
  ],
  "connections": {
    "Every Minute": {
      "main": [
        [
          {
            "node": "Call Trading Brain",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}
